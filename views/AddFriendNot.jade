extends layout

block content
	div.container
		div.row
			div.col-sm-6.col-md-4.col-md-offset-4
				#user
					h1.text-center.login-title= 'You become friends with '+info.fremail
						a(href='/users', class='text-center new-account') View list user
					#message
					if message
						h1.text-center.error-message #{message}